<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
            http://www.springframework.org/schema/context
         http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<!-- <context:component-scan base-package="com.xworkz.restaurent"/> -->



	<bean class="java.util.ArrayList" id="list">
		<constructor-arg name="ownerName">
			<list>
				<value>Raghu</value>
				<value>Agarwal</value>
				<value>Chintu</value>
			</list>
		</constructor-arg>
	</bean>

	<bean class="java.lang.Integer" id="int">
		<constructor-arg name="noOfWorkers" value="10" />


	</bean>
	<bean class="java.util.LinkedHashSet" id="set">
		<constructor-arg name="set">
			<set>
				<value>Agarwal bhavan</value>
				<value>Shastri Sagar</value>
				<value>Ashoka hotel</value>
			</set>
		</constructor-arg>
	</bean>


	<bean class="java.util.LinkedHashMap" id="map">
		<constructor-arg name="address">
			<map>
				<entry key="1" value="Rajajinagar" />
				<entry key="2" value="Vijayanagar" />
				<entry key="3" value="jalahalli Cross" />
			</map>
		</constructor-arg>
	</bean>



	<bean class="com.xworkz.restaurent.Restaurent" id="res">
		<constructor-arg name="name" ref="set" />
		<constructor-arg name="noOfWorkers" ref="int" />
		<constructor-arg name="noOfBranches" value="9" />
		<constructor-arg name="address" ref="map" />
		<constructor-arg name="ownerName" ref="list" />

	</bean>

</beans>
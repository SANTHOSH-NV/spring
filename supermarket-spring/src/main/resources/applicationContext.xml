<?xml version="1.0" encoding="UTF-8"?>


	<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
           
           
     <bean class="com.xworkz.supermarket.Address" id="addressId">
    	<constructor-arg name="buildingNo" value="24"/>
        <constructor-arg name="doorNo" value="109"/>
        <constructor-arg name="area" value="Peenya"/>
        <constructor-arg name="city" value="Bangalore"/>
        <constructor-arg name="pincode" value="560058"/>
    </bean>
    
    
    
    <bean class="com.xworkz.supermarket.Worker" id="workerId">
    	<constructor-arg name="name" value="Chintu"/>
        <constructor-arg name="age" value="25"/>
        <constructor-arg name="id" value="2345"/>
        <constructor-arg name="phoneNo" value="9986533211"/>
    </bean>
    
    <bean class="com.xworkz.supermarket.SuperMarket" id="market">
    	<constructor-arg name="name" value="More"/>
        <constructor-arg name="noOFCounters" value="5"/>
        <constructor-arg name="address" ref="addressId"/>
        <constructor-arg name="noOfBranches" value="10"/>
        <constructor-arg name="worker" ref="workerId"/>
    </bean>
    
    
    
    
           
           </beans>
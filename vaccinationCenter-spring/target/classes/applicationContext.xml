<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="com.xworkz.vaccine.Address" id="addressId">
		<constructor-arg name="buildingNo" value="40" />
		<constructor-arg name="area" value="Rajajinagar" />
		<constructor-arg name="city" value="Bangalore" />
		<constructor-arg name="pincode" value="560021" />
	</bean>



	<bean class="com.xworkz.vaccine.Doctor" id="doctorId">
		<constructor-arg name="name" value="Appaya" />
		<constructor-arg name="age" value="23" />
		<constructor-arg name="gender" value="Male" />
		<constructor-arg name="id" value="5544" />
		<constructor-arg name="phoneNo" value="9654564651" />
	</bean>

	<bean class="com.xworkz.vaccine.VaccinationCenter" id="vaccine">
		<constructor-arg name="centerName"
			value="Government School" />
		<constructor-arg name="doctors" ref="doctorId" />
		<constructor-arg name="noOfDoctors" value="4" />
		<constructor-arg name="address" ref="addressId" />
		<constructor-arg name="vaccines" value="Covidshield" />
		<constructor-arg name="noOfVaccinesAvailable"
			value="60" />


	</bean>

</beans>